{
  "presets": [
    ["env", {
      "targets": {
        "browsers": [
          "last 2 versions",
          "ie >= 10",
          "> 1% in CH",
          "not OperaMini all",
          "not OperaMobile > 0"
        ]
      }
    }]
  ],
  "plugins": [
    "transform-proto-to-assign",
    ["transform-es2015-classes", {
      "loose": true
    }]
  ]
}
