    "url": "https://github.com/brandleadership/partystreusel.git"
  },
  "bugs": {
    "url": "https://github.com/brandleadership/partystreusel/issues"
  },
  "scripts": {
    "start": "npm-run-all --parallel clean:public clean:build fonts css gulp:dev webpack:dev fractal",
    "build": "npm-run-all clean:public clean:build fonts css gulp:build webpack:build fractal:build",
    "fractal": "fractal start --sync",
    "fractal:build": "fractal build",
    "gulp:dev": "cross-env NODE_ENV=development gulp",
    "gulp:build": "cross-env NODE_ENV=production gulp",
    "webpack:dev": "cross-env NODE_ENV=development webpack --progress --watch",
    "webpack:build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "css": "del-cli public/css && npm run sass && npm run autoprefixer",
    "sass": "node-sass --include-path 'node_modules' --output-style compressed --source-map true -o public/css patterns",
    "autoprefixer": "postcss -m true -u autoprefixer -r public/css/*.css",
    "fonts": "cpx 'patterns/fonts/*.{eot,woff,woff2,ttf,svg}' public/fonts",
    "clean:public": "del-cli public/*",
    "clean:build": "del-cli build/*",
    "deploy": "npm-run-all gulp webpack:build fractal:build deploy:github",
    "deploy:github": "gh-pages -d build",
    "lint": "stylelint 'patterns/**/*.scss' --syntax scss",
    "test": "npm run sass & npm run lint"
  },
  "engines": {
    "node": ">=4"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-es2015-classes": "^6.24.1",
    "babel-plugin-transform-proto-to-assign": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "cpx": "^1.5.0",
    "cross-env": "^5.1.1",
    "del-cli": "^1.1.0",
    "eslint": "^4.10.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "gh-pages": "^1.0.0",
    "gulp": "^3.9.1",
    "gulp-if": "^2.0.2",
    "gulp-imagemin": "^3.4.0",
    "gulp-include": "^2.3.1",
    "gulp-rename": "^1.2.2",
    "gulp-svg-symbols": "^2.0.2",
    "gulp-uglify": "^3.0.0",
    "node-sass": "^4.6.0",
    "npm-run-all": "^4.1.1",
    "postcss-cli": "^4.1.1",
    "run-sequence": "^2.2.0",
    "stylelint": "^8.2.0",
    "stylelint-config-partystreusel": "^2.1.0",
    "susy": "^2.2.12",
    "webpack": "^3.8.1"
  },
  "dependencies": {
    "@frctl/fractal": "^1.1.7",
    "array-from": "^2.1.1",
    "autoprefixer": "^7.1.6",
    "browserslist": "^2.7.0",
    "element-closest": "^2.0.2",
    "flickity": "^2.0.10",
    "lazysizes": "^4.0.1",
    "moveto": "^1.7.1",
    "normalize.css": "^7.0.0",
    "picturefill": "^3.0.2",
    "tabbyjs": "^11.2.0"
  }
}
