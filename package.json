  "scripts": {
    "start": "npm-run-all clean imagemin svgsprite --parallel webpack:dev fractal:start",
    "build": "npm-run-all clean imagemin svgsprite --parallel webpack:build --sequential fractal:build",
    "webpack:dev": "webpack --mode development --watch",
    "webpack:build": "webpack --mode production --progress --hide-modules --optimize-minimize",
    "fractal:start": "fractal start --sync --watch",
    "fractal:build": "fractal build",
    "clean": "del-cli public/* build/*",
    "imagemin": "imagemin patterns/images/*.{jpg,png,gif,svg}/ --out-dir=public/images",
    "svgsprite": "svg-sprite --svg-xmldecl false --symbol --symbol-sprite 'icon-sprite.svg' --sx true patterns/images/./**/*.svg --symbol-dest public/images/ --symbol-example-dest ../../patterns/01-atoms/icons/all-icons.hbs --symbol-example-template patterns/01-atoms/icons/_sprite-preview-template.html",
    "lint": "stylelint 'patterns/**/*.scss' --syntax scss",
    "test": "run-s css:build lint",
    "deploy": "npm-run-all build deploy:github",
    "deploy:github": "gh-pages -d build"
  },
  "engines": {
    "node": ">=10"
  },
  "devDependencies": {
    "@frctl/fractal": "^1.1.7",
    "autoprefixer": "^9.6.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-es2015-classes": "^6.24.1",
    "babel-plugin-transform-proto-to-assign": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "browserslist": "^4.6.2",
    "cpx": "^1.5.0",
    "css-loader": "^3.0.0",
    "del-cli": "^2.0.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.17.3",
    "extract-loader": "^3.1.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^4.0.0",
    "gh-pages": "^2.0.1",
    "imagemin-cli": "^4.0.1",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "stylelint": "^10.1.0",
    "stylelint-config-partystreusel": "^2.1.0",
    "svg-sprite": "^1.5.0",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.4"
  },
